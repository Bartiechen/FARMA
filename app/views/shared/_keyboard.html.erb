<div id="keyboard">
  <div class="jax" id="MathOutput">$${}$$</div>
  <!-- Screen and clear key -->
  <div class="top">
    <input id="question_{{id}}_response" type="text" class="screen" value="" name="answer[response]">
    <label for="question_response">Testar Resposta</label>
    <button class="clear" data="C">C</button>
    <!-- <input type="hidden" class="screen" id="MathInput"></input> -->
    <!-- <button class="clear material-icons" data="<">keyboard_arrow_left</button>
    <button class="clear material-icons" data=">">keyboard_arrow_right</button> -->
  </div>

  <div class="keys">
    <!-- operators and other keys -->
    <button class="waves" data="7">7</button>
    <button data="8">8</button>
    <button data="9">9</button>
    <button class="operator"  data="\over">÷</button>
    <button data="4">4</button>
    <button data="5">5</button>
    <button data="6">6</button>
    <button class="operator"  data="*">x</button>
    <button data="1">1</button>
    <button data="2">2</button>
    <button data="3">3</button>
    <button class="operator" data="-">-</button>
    <button data="0">0</button>
    <button data=".">.</button>
    <button class="eval" data="=">=</button>
    <button class="operator" data="+">+</button>
  </div>
  <div class="keys" id="more">
    <button data="\sqrt{}">√</button>
    <button data="{">(</button>
    <button data="}">)</button>
    <button class="operator mdi mdi-matrix" data="\left[\begin{matrix}\end{matrix}\right]"></button>
    <button><sup>y</sup>√</button>
    <button data="^2">𝑥<sup>2</sup></button>
    <button data="^">𝑥<sup>y</sup></button>
    <button class="operator material-icons" data="\\">keyboard_return</button>
  </div>
</div>
